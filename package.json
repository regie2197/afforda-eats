{
  "name": "afforda-eats",
  "version": "1.0.0",
  "type": "module",
  "description": "Jolly Bus / Konbini Web Application made with NextJS &amp; Supabase DB and Cypress Test Automation",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run client\"",
    "build": "npm --prefix client run build",
